  "bugs": {
    "url": "https://github.com/eladmoshe/offline-cache/issues"
  },
  "homepage": "https://github.com/eladmoshe/offline-cache#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
